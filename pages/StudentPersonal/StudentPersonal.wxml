<view class="view_contain">

  <!-- 第一部分 -->
  <image class="header_bg" mode="widthFix" src="../../static/images/index.png"></image>
  <view class="header" style="{{header_style}}">
  </view>



  <view class="view_1">
    <view class="view_image_text">
      <text>{{name}}</text>
      <text>学号：{{Id}}</text>
      <text>{{college}}{{major}}{{stuClass}}</text>
    </view>
  </view>

  <view class="big3">
    <view wx:for="{{info}}" wx:key="item" class="view_3 {{item.class}}" bindtap="TypeChange">   
        <view class="list-item">
          <text class="item-text">{{item.title}}</text>
        </view>
    </view>
  </view>
</view>