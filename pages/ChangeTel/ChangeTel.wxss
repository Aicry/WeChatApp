/* pages/ChangeTel/ChangeTel.wxss *//* pages/ChangeMsg/ChangeMsg.wxss */
page{ 
    background: #f5f5f5;
  } 
  .container {
    display: flex;
    flex-direction: column;
  }
   
  .header_bg {
    z-index: -1;
    width: 100%;
  }
  
  .header {
    position: absolute;
  }
   
  /*登录图片*/ 
  .login-icon{ 
    flex: none; 
  } 
  .login-img{ 
    width: 750rpx; 
  } 
  .view_1 {
    display: flex;
    justify-content: center;
    width: 100%;
    height: 200rpx;
    margin-top: -10rpx;
    background: rgb(52,120,247);
  }
  
  /*表单内容*/ 
  .login-from { 
    margin-top: 20px; 
    flex: auto; 
    height:100%; 
  } 
   
  .inputView { 
    background-color: #fff; 
    line-height: 80px; 
  } 
  /*输入框*/ 
  .nameImage, .keyImage { 
    margin-left: 22px; 
    width: 20px; 
    height: 20px 
  } 
   
  .loginLab { 
    margin: 15px 15px 15px 10px; 
    color: #545454; 
    font-size: 20px 
  } 
  .inputText { 
    flex: block; 
    float: right; 
    text-align: right; 
    margin-right: 22px; 
    margin-top: 11px; 
    color: #cccccc; 
    font-size: 14px } 
   
    .view_image_text {
      width: 100%;
      display: flex;
      justify-content: left;
      flex-direction: column; 
        /* align-items: center; */
      color: white;
      margin-left: 40rpx;
      margin-top: 80rpx;
    }

  .line { 
    width: 100%; 
    height: 1px; 
    background-color: #cccccc; 
    margin-top: 1px; 
  } 
  /*按钮*/ 
  .changeBtnView { 
    width: 100%; 
    height: auto; 
    background-color: #f2f2f2; 
    margin-top: 0px; 
    margin-bottom: 0px; 
    padding-bottom: 0px; 
  } 
  .returnBtnView { 
    width: 100%; 
    height: auto; 
    background-color: #f2f2f2; 
    margin-top: 0px; 
    margin-bottom: 0px; 
    padding-bottom: 0px; 
  } 
  .changeBtn { 
    width: 80%; 
    margin-top: 35px; 
  } 
  .returnBtn{
    width: 80%; 
    margin-top: 35px; 
  }
  
  .header {
    position: absolute;
    display: flex;
    align-items: center;
  }
  
  .return_svg {
    height: 40rpx;
    width: 40rpx;
  }
  
  .details_list {
    display: flex;
    flex-direction: column;
    align-items: center;
  }
  
  .no_data__line {
  font-size: 28rpx;
  margin-top: 40rpx;
  line-height: 1.5;
  color: #7e8da6;
  text-align: center;
  }
  
  .details_item {
    width: 690rpx;
    height: 220rpx;
    margin: 20rpx auto 0;
    background: #fff;
    border-radius: 18rpx;
    display: flex;
    align-items: center;
  }
  
  .details_item image {
    width: 160rpx;
    height: 160rpx;
    margin: 40rpx 20rpx;
  }
  
  .item__text {
  width: 460rpx;
  display: flex;
  flex-direction: column;
  }
  
  .item__bodyTemp {
  font-size: 32rpx;
  color: #0c112b;
  display: block;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  }
  
  .item__status {
  margin-top: 26rpx;
  font-size: 24rpx;
  color: #7e8da6;
  font-weight: 400;
  }
  
  .type_group {
    display: flex;
    justify-content: center;
    align-items: center;
  }
  
  